{
  "name": "cubo-ui-srcs",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "deploy": "gh-pages-deploy",
    "test": "ng test",
    "e2e": "ng e2e",
    "lint": "npm run lint:ts && npm run lint:css",
    "lint:ts": "tslint -c tslint.json 'src/**/*.ts'",
    "lint:css": "sass-lint -c sass-lint.json 'src/**/*.scss' -v -q",
    "fix:ts": "tslint -c tslint.json 'src/**/*.ts' --fix",
    "prepush": "npm run lint",
    "postinstall": "cd ./src/lib; npm link; cd ../../; npm link cubo-ui"
  },
  "gh-pages-deploy": {
    "staticpath": "dist",
    "prep": [
      "build"
    ],
    "noprompt": true
  },
  "dependencies": {
    "@angular/animations": "^6.0.0-rc.5",
    "@angular/cdk": "^6.0.0-rc.12",
    "@angular/common": "^6.0.0-rc.5",
    "@angular/compiler": "^6.0.0-rc.5",
    "@angular/core": "^6.0.0-rc.5",
    "@angular/flex-layout": "github:angular/flex-layout-builds",
    "@angular/forms": "^6.0.0-rc.5",
    "@angular/http": "^6.0.0-rc.5",
    "@angular/material": "^6.0.0-rc.12",
    "@angular/platform-browser": "^6.0.0-rc.5",
    "@angular/platform-browser-dynamic": "^6.0.0-rc.5",
    "@angular/router": "^6.0.0-rc.5",
    "@ngx-meta/core": "^6.0.0-rc.1",
    "core-js": "^2.5.4",
    "highlight.js": "^9.12.0",
    "rxjs": "^6.0.0-rc.0",
    "zone.js": "^0.8.24"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.5.0",
    "@angular/cli": "~6.0.0-rc.5",
    "@angular/compiler-cli": "^6.0.0-rc.5",
    "@angular/language-service": "^6.0.0-rc.5",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~9.6.6",
    "codelyzer": "~4.3.0",
    "cubo-schematics": "^1.0.3",
    "gh-pages-deploy": "^0.5.0",
    "husky": "^0.14.3",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^1.0.0",
    "protractor": "~5.3.0",
    "rupture-sass": "^0.2.3",
    "sass-lint": "^1.12.1",
    "ts-node": "~6.0.0",
    "tslint": "~5.9.1",
    "typescript": "^2.7.2"
  }
}
